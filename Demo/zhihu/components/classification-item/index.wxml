<navigator class='classification-item' url='{{jumpPath}}?tag_id={{tag_id}}&title={{title}}'>
  <view>
    <image src='{{imgSrc}}' mode='aspectFill' binderror='_imageError'></image>
  </view>
  <view class='content'>
    <view class='info'>
      <text class='title'>{{title}}</text>
      <text class='problem-count'>{{problemCount}}个问题</text>
    </view>
    <image src='/images/arrow.png'></image>
  </view>
</navigator>